{
  "name": "eosc-pl-commons-components",
  "version": "1.0.0",
  "description": "Library containing the custom UI components of the EOSC services. Uses the JS scripts, and the SCSS styles for consistent visualization and events triggering.",
  "scripts": {
    "clean": "rm -rf node_modules package-lock.json yarn.lock",
    "reinstall": "yarn run clean && yarn install",
    "start": "NODE_ENV=development gulp serve",
    "build": "NODE_ENV=production gulp build --env env/env.production.js",
    "check:dependencies": "npm i depcheck -g && npx depcheck",
    "test": "NODE_ENV=test jest --config=jest.config.js",
    "test:coverage": "NODE_ENV=test jest --coverage --config=jest.config.js",
    "lint": "npx eslint \"src/**/*.{js,jsx}\" \"core/**/*.{js,jsx}\"",
    "lint:fix": "npx eslint \"src/**/*.{js,jsx}\" \"core/**/*.{js,jsx}\" --fix",
    "build:docs": "npx styleguidist build --config styleguide.config.js"
  },
  "devDependencies": {
    "@babel/core": "^7.28.0",
    "@babel/plugin-proposal-decorators": "^7.28.0",
    "@babel/plugin-transform-react-jsx": "^7.27.1",
    "@babel/preset-env": "^7.28.0",
    "@babel/preset-react": "^7.27.1",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.31.0",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/preact": "^3.2.3",
    "@testing-library/user-event": "^14.5.2",
    "@types/js-cookie": "^3.0.6",
    "babel-jest": "^28.1.3",
    "babel-loader": "^10.0.0",
    "bootstrap": "^5.3.7",
    "browser-sync": "^3.0.4",
    "compression": "^1.8.0",
    "copy-webpack-plugin": "^13.0.0",
    "css-loader": "^7.1.2",
    "del": "^8.0.0",
    "eslint": "^9.31.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "execa": "^9.6.0",
    "fancy-log": "^2.0.0",
    "file-loader": "^6.2.0",
    "globals": "^16.3.0",
    "gulp": "^5.0.1",
    "gulp-if": "^3.0.0",
    "gulp-rename": "^2.1.0",
    "gulp-sass": "^6.0.1",
    "gulp-sourcemaps": "^3.0.0",
    "husky": "^9.1.7",
    "jest": "^28.1.3",
    "jest-environment-jsdom": "^28.1.3",
    "node-notifier": "^10.0.1",
    "preact-render-to-string": "^6.5.13",
    "prettier": "^3.6.2",
    "react-styleguidist": "^13.1.4",
    "sass": "^1.89.2",
    "sass-loader": "^16.0.5",
    "style-loader": "^4.0.0",
    "terser-webpack-plugin": "^5.3.14",
    "vinyl-named": "^1.1.0",
    "webpack": "^5.100.2",
    "webpack-stream": "^7.0.0",
    "window-resizeto": "^0.0.2",
    "yargs-parser": "^22.0.0"
  },
  "dependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@popperjs/core": "^2.11.8",
    "js-cookie": "^3.0.5",
    "preact": "^10.26.9",
    "prop-types": "^15.8.1",
    "react": "^19.1.0",
    "react-bootstrap": "^2.10.10",
    "react-dom": "^19.1.0",
    "react-required-if": "^1.0.3",
    "react-responsive": "^10.0.1"
  }
}
